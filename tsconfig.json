{
	"compilerOptions": {
		"module": "es2022",
		"target": "es2024",
		"moduleResolution": "bundler",
		"paths": {},
		"esModuleInterop": true,
		"skipLibCheck": true,
		"outDir": "./out",
		"declaration": true
	},
	"include": [
		"./src"
	],
	"exclude": [
		"node_modules"
	],
	"tsc-alias": {
		"resolveFullPaths": true,
		"verbose": false
	}
}